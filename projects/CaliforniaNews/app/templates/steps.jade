extends _layout

mixin theme-card (imageName, themeName, author, check)
    div.theme--card
        a.cover--image(href="#")
            img(src="/images/site-covers/#{imageName}.jpg")
        h2= themeName
        p= author
        div.select--theme.wizard
            a.blueBtn.next(href="#") Select template
            a.read--more(href="#") Read more
        input.select--theme-radio(type="radio" value="123456547645" data-theme="123" checked=(check) name="fd[theme]")

mixin backForward ()
    .back--forward.wizard
        a.blueBtn.next(href="#") Confirm and continue
        a.backBtn.previous(href="#") Back
            svg(width="21" height="16" viewBox="0 0 21 16" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink")
