mixin contentTitle (h1Tittle, paragraph)
    div.content--title.what
        h1= h1Tittle
        p= paragraph


mixin notes (heading, date, heading2, heading3, paragraph1, paragraph2, paragraph3)
    div.content--notes
        h1= heading
        span= date
        h2= heading2
        h3= heading3
        p= paragraph1
        p= paragraph2
        p= paragraph3

block content
    main
        div#rootWizard
            div.steps--container
                h1
                    | Step
                    span.step--count  1
                    |  of 4
                p You don't need be a designer. We've developed a wide range of blocks. Contemporary look and high flexibility makes them handy and applicable for any kind of content.
                div.progress--bar
                    ul.prog
                        li.active Choose a template
                            a(href="#step-1" data-toggle="tab")
                        li Select Plugins
                            a(href="#step-2" data-toggle="tab")
                        li Authorization
                            a(href="#step-3" data-toggle="tab")
                        li Download Site
                            a(href="#finish" data-toggle="tab")
            form(method="POST")#steps--container-wrappper.tab-content
                section.content.content--tehemes.tab-pane#step-1
                    div.theme--form
                        +theme-card('cover1', 'Daily Insight', 'by Yam Chhetri', 'checked')
                        +theme-card('cover2', 'ProfitMag', 'by Rigorous Themes')
                        +theme-card('cover3', 'RainbowNews', 'by 99colorthemes')
                        +theme-card('cover4', 'Mercia', 'by MH Themes')
                        +theme-card('cover5', 'MeisterMag', 'by wpion')
                        +theme-card('cover6', 'SuperNews', 'by Acme Themes')
                        +theme-card('cover7', 'Metro Magazine', 'by Rara Theme')

                section.content.content--plugins.tab-pane#step-2
